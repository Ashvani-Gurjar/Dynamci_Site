   <div class="container-fluid mt-5 contact-style" id="contactid">
     <div class="row">
      <div class="col-md-10 col-12 mx-auto">
        <h1 class="text-center main-heading">Contact Us</h1>
        <p class="text-center sub-heading">We will be happy to help you</p>


        <div class="row">
          <div class="col-md-6 col-12 mx-auto">

            <form action="/contact" method="POST">
            <div class="form-group">
              <label for="expamleInputPassword1">Name</label>
              <input type="text" name="name" id="exampleInputPassword1" class="form-control" itemid="exampleInputPassword1" placeholder="Enter your name">
            </div>
            
            <div class="form-group">
              <label for="expamleInputEmail1">Email Addreass</label>
              <input type="email" name="email" id="expamleInputEmail1" class="form-control" itemid="exampleInputPassword1" placeholder="Enter you email" aria-describedby="emailHelp">
              <small class="form-text text-muted" id="emailHelp">We'wll never shar your email with anyon else</small>
            </div>
            <div class="form-group">
              <label for="expamleInputPassword1">Phone</label>
              <input type="number" name="phone" id="exampleInputPassword1" class="form-control" placeholder="Phone number">
            </div>

            <div class="form-group">
              <label for="expamleInputPassword1">Messages</label>
              <input type="text" name="message" id="exampleInputPassword" class="form-control" placeholder="Message">
            </div>
            <button type="submit" class="btn-style btn-primary">Submit</button>
            
            </form>


          </div>
        </div>
      </div>
